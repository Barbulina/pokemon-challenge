{
  "name": "pokemon-challenge",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest --watch",
    "test:ci": "jest --ci",
    "prepare": "husky install",
    "tsc": "tsc --noEmit",
    "lint": "eslint src",
